{
  "version": "v0.1.3",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.3.5",
    "antd": "^4.10.2",
    "highcharts": "^8.2.2",
    "highcharts-react-official": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-highlight-words": "^0.16.0",
    "typescript": "^4.1.2",
    "umi": "^3.3.5",
    "yorkie": "^2.0.0"
  }
}
