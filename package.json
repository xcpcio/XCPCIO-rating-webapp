{
  "version": "v0.1.9",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{less,md,json,ts,tsx}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.31.5",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "antd": "^4.17.3",
    "highcharts": "^9.0.0",
    "highcharts-react-official": "^3.1.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-highlight-words": "^0.17.0",
    "typescript": "^4.5.2",
    "umi": "^3.5.20",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/react-highlight-words": "^0.16.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11"
  },
  "engines": {
    "node": ">=16.11.0"
  }
}
