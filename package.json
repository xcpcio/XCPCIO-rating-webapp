{
  "version": "v0.1.8",
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{less,md,json,ts,tsx}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.13",
    "antd": "^4.16.7",
    "highcharts": "^9.0.0",
    "highcharts-react-official": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "react-highlight-words": "^0.16.0",
    "typescript": "^4.3.5",
    "umi": "^3.5.13",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": ">=14.15.5"
  },
  "devDependencies": {
    "@types/react-highlight-words": "^0.16.3"
  }
}
